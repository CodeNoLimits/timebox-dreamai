# TimeBox by DreamAI - Production Environment Variables

# Application Configuration
EXPO_PUBLIC_APP_NAME=TimeBox by DreamAI
EXPO_PUBLIC_APP_VERSION=3.0.0
EXPO_PUBLIC_ENVIRONMENT=production

# API Configuration
EXPO_PUBLIC_API_URL=https://api.timebox-dreamai.com
EXPO_PUBLIC_API_VERSION=v1
EXPO_PUBLIC_API_TIMEOUT=10000

# Authentication
EXPO_PUBLIC_AUTH_DOMAIN=auth.timebox-dreamai.com

# Analytics & Monitoring
EXPO_PUBLIC_GA_TRACKING_ID=G-XXXXXXXXXX

# Premium Features
EXPO_PUBLIC_PREMIUM_PRICE=1.99

# Team Collaboration
EXPO_PUBLIC_WEBSOCKET_URL=wss://ws.timebox-dreamai.com
EXPO_PUBLIC_PUSHER_CLUSTER=us2

# Export & Storage
EXPO_PUBLIC_AWS_REGION=us-east-1
EXPO_PUBLIC_S3_BUCKET=timebox-exports-prod
EXPO_PUBLIC_CLOUDFRONT_URL=https://cdn.timebox-dreamai.com

# Feature Flags
EXPO_PUBLIC_ENABLE_AI_INSIGHTS=true
EXPO_PUBLIC_ENABLE_BINAURAL_BEATS=true
EXPO_PUBLIC_ENABLE_TEAM_COLLABORATION=true
EXPO_PUBLIC_ENABLE_EXPORT_FEATURES=true
EXPO_PUBLIC_ENABLE_PREMIUM_FEATURES=true

# Debug Configuration
EXPO_PUBLIC_DEBUG_MODE=false
EXPO_PUBLIC_LOG_LEVEL=error
EXPO_PUBLIC_ENABLE_PERFORMANCE_MONITORING=true

# Social Media & Sharing
EXPO_PUBLIC_TWITTER_HANDLE=@TimeBoxAI
EXPO_PUBLIC_LINKEDIN_URL=https://linkedin.com/company/dreamai

# Support & Documentation
EXPO_PUBLIC_SUPPORT_EMAIL=support@dreamai.com
EXPO_PUBLIC_DOCS_URL=https://docs.timebox-dreamai.com
EXPO_PUBLIC_FEEDBACK_URL=https://feedback.timebox-dreamai.com